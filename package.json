{
  "name": "bitwages",
  "version": "1.0.0",
  "description": "Show bitcoin prices",
  "main": "index.js",
  "scripts": {
    "start": "serve .",
    "manual-deploy": "./node_modules/.bin/now rm bitwages -s -y && ./node_modules/.bin/now --public && ./node_modules/.bin/now alias -r rules.json",
    "automatic-deploy": "./node_modules/.bin/now rm bitwages -s -y -t ${zeitToken} && ./node_modules/.bin/now --public -t ${zeitToken} && ./node_modules/.bin/now alias -t ${zeitToken} -r rules.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "serve": "latest"
  },
  "now": {
    "alias": "bitwages",
    "type": "static"
  },
  "devDependencies": {
    "now": "^8.3.4"
  }
}
